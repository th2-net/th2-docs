{
  "name": "th2-docs-gridsome",
  "private": true,
  "scripts": {
    "build": "npm run plugins:compile && gridsome build",
    "prod:local": "http-server ./dist",
    "dev": "npm run plugins:compile && gridsome develop",
    "explore": "npm run plugins:compile && gridsome explore",
    "plugins:compile": "tsc --project src/plugins/tsconfig.json",
    "test:unwelcome-words": "ts-node --project tsconfig.scripts.json .github/scripts/unwelcome-words.ts",
    "cypress:prepare-fixtures": "ts-node --project cypress/tsconfig.json cypress/scripts/downloadSitemaps.ts",
    "cypress:open": "cypress open",
    "cypress:test-dev": "cypress run --spec \"cypress/e2e/development.cy.ts\"",
    "cypress:test-prebuilt-prod": "cypress run --spec \"cypress/e2e/production.local.cy.ts\"",
    "cypress:test-prod": "cypress run --spec \"cypress/e2e/production.cy.ts\""
  },
  "dependencies": {
    "@gridsome/vue-remark": "^0.2.6",
    "@vue/composition-api": "^1.6.2",
    "clipboard": "^2.0.10",
    "echarts": "^5.3.3",
    "gridsome": "^0.7.0",
    "marked": "^4.0.17",
    "vue-axios": "^3.4.1",
    "vue-echarts": "^6.1.0",
    "vuetify": "^2.6.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@akebifiky/remark-simple-plantuml": "^1.0.2",
    "@gridsome/plugin-sitemap": "^0.4.0",
    "@gridsome/remark-prismjs": "^0.5.0",
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/transformer-remark": "^0.6.4",
    "@saber2pr/types-github-api": "^0.0.7",
    "@types/cli-progress": "^3.11.0",
    "@types/extract-domain": "^2.3.1",
    "@types/js-levenshtein": "^1.1.1",
    "badwords-list": "^1.0.0",
    "cli-progress": "^3.11.2",
    "cypress": "^10.11.0",
    "deepmerge": "^4.2.2",
    "extract-domain": "^2.4.8",
    "extract-urls": "^1.3.2",
    "fibers": "^5.0.1",
    "gridsome-plugin-robots-txt": "^1.0.2",
    "gridsome-plugin-typescript": "^0.4.0",
    "hast-util-select": "^5.0.2",
    "hast-util-to-html": "^6.0.2",
    "hastscript": "^3.0.0",
    "http-server": "^14.1.1",
    "js-levenshtein": "^1.1.6",
    "markdown-headers": "^0.0.1",
    "markdown-to-txt": "^2.0.1",
    "mdast-util-to-hast": "^4.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "node-html-parser": "^5.3.3",
    "octokit": "^2.0.10",
    "remark-emoji": "^2.0.1",
    "remark-prism": "^1.3.6",
    "sass": "^1.32.6",
    "sass-loader": "^7.3.1",
    "ts-loader": "~6.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.5.5",
    "unist-builder": "^1.0.4",
    "unist-util-visit": "^1.4.0",
    "vuetify-loader": "^1.7.3",
    "webpack-node-externals": "^3.0.0",
    "x2js": "^3.4.4"
  }
}
